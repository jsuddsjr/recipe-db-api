    "recipeIngredient": [
        "3 avocados - peeled, pitted, and mashed",
        "1 lime, juiced",
        "1 teaspoon salt",
        "2 roma (plum) tomatoes, diced",
        "0.5 cup diced onion",
        "3 tablespoons chopped fresh cilantro",
        "1 teaspoon minced garlic",
        "1 pinch ground cayenne pepper"
    ],
    "recipeInstructions": [
        {
            "@type": "HowToStep",
            "text": "Mash avocados, lime juice, and salt together in a medium bowl; mix in tomatoes, onion, cilantro, and garlic. Stir in cayenne pepper."
        },
        {
            "@type": "HowToStep",
            "image": [
                {
                    "@type": "ImageObject",
                    "url": "https://www.allrecipes.com/thmb/z3kI3C-Z63q3UHqwqbr53SCU1Y4=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/14231_guacamole_Rita-1x1-1-6797bd34d19b4f88a210f2086ca1156f.jpg"
                }
            ],
            "text": "Serve immediately, or cover and refrigerate for 1 hour for improved flavor."
        }
    ],
    "recipeYield": [
        "4"
    ],
    "totalTime": "PT15M",
